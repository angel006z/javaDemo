			AND a.memberId like "%"#{memberId}"%"
		</if>
		<if test="orderNo != null and orderNo!=''">
			AND a.orderNo like "%"#{orderNo}"%"
		</if>
		LIMIT
		#{pagination.pageIndex},#{pagination.pageSize}
	</select>
	
