    <update id="update" parameterType="com.meida.front.pay.po.MemberInfo">
        update tb_member
        <set>
                            <if test="nickname!=null">
                    nickname=#{nickname},
                </if>
                            <if test="account!=null">
                    account=#{account},
                </if>
                            <if test="telphone!=null">
                    telphone=#{telphone},
                </if>
                            <if test="password!=null">
