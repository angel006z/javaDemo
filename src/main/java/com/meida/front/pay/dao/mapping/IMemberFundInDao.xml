	<resultMap id="myResultMap"
		type="com.meida.front.pay.domain.po.MemberFundIn">
		<id property="memberFundInId" column="memberFundInId"></id>
		<result property="memberId" column="memberId"></result>
		<result property="orderNo" column="orderNo"></result>
		<result property="inMoney" column="inMoney"></result>
		<result property="inDate" column="inDate"></result>
		<result property="inWay" column="inWay"></result>
		<result property="createDate" column="createDate"></result>
		<result property="operateDate" column="operateDate"></result>
		<result property="operatorId" column="operatorId"></result>
		<result property="isValid" column="isValid"></result>
		<result property="remark" column="remark"></result>
	</resultMap>

	<select id="getObjectById" parameterType="java.lang.Long"
		resultMap="myResultMap">
		select
		memberFundInId,memberId,orderNo,inMoney,inDate,inWay,createDate,operateDate,operatorId,isValid,remark
		from tb_member_fund_in where memberFundInId=#{id}
	</select>
	
	<select id="getObjectByOrderNo" parameterType="java.lang.String"
		resultMap="myResultMap">
		select
		memberFundInId,memberId,orderNo,inMoney,inDate,inWay,createDate,operateDate,operatorId,isValid,remark
		from tb_member_fund_in where orderNo=#{orderNo}
	</select>
	
	<select id="isExistOrderNo" parameterType="java.lang.String"
		resultType="java.lang.Long">
		select count(*) from tb_member_fund_in where
		orderNo=#{orderNo}
	</select>


	<select id="getListByAll" resultMap="myResultMap">
		select
		memberFundInId,memberId,orderNo,inMoney,inDate,inWay,createDate,operateDate,operatorId,isValid,remark
		from tb_member_fund_in
	</select>


	<select id="getListByValid" resultMap="myResultMap">
		select
		memberFundInId,memberId,orderNo,inMoney,inDate,inWay,createDate,operateDate,operatorId,isValid,remark
		from tb_member_fund_in where isValid=1
	</select>

	<resultMap id="myPageResultMap"
		type="com.meida.front.pay.domain.vo.MemberFundInVo">
		<id property="memberFundInId" column="memberFundInId"></id>
		<result property="memberId" column="memberId"></result>
		<result property="orderNo" column="orderNo"></result>
		<result property="inMoney" column="inMoney"></result>
		<result property="inDate" column="inDate"></result>
		<result property="inWay" column="inWay"></result>
		<result property="createDate" column="createDate"></result>
		<result property="operateDate" column="operateDate"></result>
		<result property="operatorId" column="operatorId"></result>
		<result property="isValid" column="isValid"></result>
		<result property="remark" column="remark"></result>
	</resultMap>
	
	<select id="getListWhereByPage"
		parameterType="com.meida.front.pay.domain.dto.MemberFundInListDto"
		resultMap="myPageResultMap">
		select
		a.memberFundInId,a.memberId,a.orderNo,a.inMoney,a.inDate,a.inWay,a.createDate,a.operateDate,a.operatorId,a.isValid,a.remark
		from tb_member_fund_in as a
		where a.isValid=1
		<if test="memberId != null and memberId!=''">
			AND a.memberId like "%"#{memberId}"%"
		</if>
		<if test="orderNo != null and orderNo!=''">
			AND a.orderNo like "%"#{orderNo}"%"
		</if>
		LIMIT
		#{pagination.pageIndex},#{pagination.pageSize}
	</select>
	
	<select id="getListByPage"
		parameterType="com.meida.front.pay.domain.dto.MemberFundInListDto"
		resultMap="myPageResultMap">
		select
		a.memberFundInId,a.memberId,a.orderNo,a.inMoney,a.inDate,a.inWay,a.createDate,a.operateDate,a.operatorId,a.isValid,a.remark
		from tb_member_fund_in as a
		where a.isValid=1
		<if test="memberId != null and memberId!=''">
			AND a.memberId like "%"#{memberId}"%"
		</if>
		<if test="orderNo != null and orderNo!=''">
			AND a.orderNo like "%"#{orderNo}"%"
		</if>
		LIMIT
		#{pagination.pageIndex},#{pagination.pageSize}
	</select>


	<select id="getTotalRecord"
		parameterType="com.meida.front.pay.domain.dto.MemberFundInListDto"
		resultType="java.lang.Long">
		select count(*) from tb_member_fund_in
		where isValid=1
		<if test="memberId != null and memberId!=''">
			AND memberId like "%"#{memberId}"%"
		</if>
		<if test="orderNo != null and orderNo!=''">
			AND orderNo like "%"#{orderNo}"%"
		</if>
		LIMIT
		#{pagination.pageIndex},#{pagination.pageSize}
	</select>


	<insert id="save"
		parameterType="com.meida.front.pay.domain.po.MemberFundIn">
		insert into tb_member_fund_in
		(memberId,orderNo,inMoney,inWay,inDate,createDate,operateDate,operatorId,isValid,remark,signature)
		values
		(#{memberId},#{orderNo},#{inMoney},#{inWay},#{inDate},
		#{createDate},#{operateDate},#{operatorId},#{isValid},#{remark},#{signature})

	</insert>


	<update id="update"
		parameterType="com.meida.front.pay.domain.po.MemberFundIn">
		update tb_member_fund_in
		<set>
			<if test="memberId!=null">
				memberId=#{memberId},
			</if>
			<if test="orderNo!=null">
				orderNo=#{orderNo},
			</if>
			<if test="inMoney!=null">
				inMoney=#{inMoney},
			</if>
			<if test="inDate!=null">
				inDate=#{inDate},
			</if>
			<if test="inWay!=null">
				inWay=#{inWay},
			</if>			
			<if test="operateDate!=null">
				operateDate=#{operateDate},
			</if>
			<if test="operatorId!=null">
				operatorId=#{operatorId},
			</if>
			<if test="remark!=null">
				remark=#{remark}
			</if>
			<if test="signature!=null">
				signature=#{signature}
			</if>
		</set>
		where memberFundInId=#{memberFundInId}
	</update>
	<update id="updateByOrderNo"
		parameterType="com.meida.front.pay.domain.po.MemberFundIn">
		update tb_member_fund_in
		<set>
			<if test="memberId!=null">
				memberId=#{memberId},
			</if>
			<if test="orderNo!=null">
				orderNo=#{orderNo},
			</if>
			<if test="inMoney!=null">
				inMoney=#{inMoney},
			</if>
			<if test="inDate!=null">
				inDate=#{inDate},
			</if>
			<if test="inWay!=null">
				inWay=#{inWay},
			</if>			
			<if test="operateDate!=null">
				operateDate=#{operateDate},
			</if>
			<if test="operatorId!=null">
				operatorId=#{operatorId},
			</if>
			<if test="remark!=null">
				remark=#{remark}
			</if>
			<if test="signature!=null">
				signature=#{signature}
			</if>
		</set>
		where orderNo=#{orderNo}
	</update>


	<delete id="deletePhysicalById" parameterType="java.lang.Long">
		delete from
		tb_member_fund_in
		where memberFundInId=#{id}
	</delete>


	<delete id="deleteLogicById" parameterType="java.lang.Long">
		update
		tb_member_fund_in set
		isValid=-1 where memberFundInId=#{id}
	</delete>

