		</set>
		where fundInId=#{fundInId}
	</update>
	<update id="updateByOrderNo"
		parameterType="com.meida.front.pay.domain.po.FundIn">
		update tb_fund_in
		<set>
			<if test="memberId!=null">
				memberId=#{memberId},
			</if>
			<if test="orderNo!=null">
				orderNo=#{orderNo},
			</if>
			<if test="inMoney!=null">
				inMoney=#{inMoney},
			</if>
			<if test="inDate!=null">
				inDate=#{inDate},
			</if>
			<if test="inWay!=null">
