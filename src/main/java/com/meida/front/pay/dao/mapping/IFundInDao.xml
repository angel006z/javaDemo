		<id property="fundInId" column="fundInId"></id>
		<result property="memberId" column="memberId"></result>
		<result property="orderNo" column="orderNo"></result>
		<result property="inMoney" column="inMoney"></result>
		<result property="inDate" column="inDate"></result>
		<result property="inWay" column="inWay"></result>
		<result property="createDate" column="createDate"></result>
		<result property="createUserId" column="createUserId"></result>
		<result property="createUser" column="createUser"></result>
		<result property="updateDate" column="updateDate"></result>
		<result property="updateUserId" column="updateUserId"></result>
		<result property="updateUser" column="updateUser"></result>
		<result property="isValid" column="isValid"></result>
		<result property="remark" column="remark"></result>
	</resultMap>

	<select id="getObjectById" parameterType="java.lang.Long"
		resultMap="myResultMap">
		select
		fundInId,memberId,orderNo,inMoney,inDate,inWay,createDate,createUserId,createUser,updateDate,updateUserId,updateUser,isValid,remark
		from tb_fund_in where fundInId=#{id}
	</select>
	
	<select id="getObjectByOrderNo" parameterType="java.lang.String"
		resultMap="myResultMap">
		select
		fundInId,memberId,orderNo,inMoney,inDate,inWay,createDate,createUserId,createUser,updateDate,updateUserId,updateUser,isValid,remark
		from tb_fund_in where orderNo=#{orderNo}
	</select>
	
	<select id="isExistOrderNo" parameterType="java.lang.String"
		resultType="java.lang.Long">
		select count(*) from tb_fund_in where
		orderNo=#{orderNo}
	</select>


	<select id="getListByAll" resultMap="myResultMap">
		select
		fundInId,memberId,orderNo,inMoney,inDate,inWay,createDate,createUserId,createUser,updateDate,updateUserId,updateUser,isValid,remark
		from tb_fund_in
	</select>


	<select id="getListByValid" resultMap="myResultMap">
		select
		fundInId,memberId,orderNo,inMoney,inDate,inWay,createDate,createUserId,createUser,updateDate,updateUserId,updateUser,isValid,remark
		from tb_fund_in where isValid=1
	</select>

	<resultMap id="myPageResultMap"
		type="com.meida.front.pay.domain.vo.FundInVo">
		<id property="fundInId" column="fundInId"></id>
		<result property="memberId" column="memberId"></result>
		<result property="orderNo" column="orderNo"></result>
		<result property="inMoney" column="inMoney"></result>
		<result property="inDate" column="inDate"></result>
		<result property="inWay" column="inWay"></result>
		<result property="createDate" column="createDate"></result>
		<result property="createUserId" column="createUserId"></result>
		<result property="createUser" column="createUser"></result>
		<result property="updateDate" column="updateDate"></result>
		<result property="updateUserId" column="updateUserId"></result>
		<result property="updateUser" column="updateUser"></result>
		<result property="isValid" column="isValid"></result>
		<result property="remark" column="remark"></result>
	</resultMap>
	
	<select id="getListWhereByPage"
		parameterType="com.meida.front.pay.domain.dto.FundInListDto"
		resultMap="myPageResultMap">
		select
		a.fundInId,a.memberId,a.orderNo,a.inMoney,a.inDate,a.inWay,a.createDate,a.createUserId,a.createUser,a.updateDate,a.updateUserId,a.updateUser,a.isValid,a.remark
		from tb_fund_in as a
		where a.isValid=1
		<if test="memberId != null and memberId!=''">
			AND a.memberId like "%"#{memberId}"%"
		</if>
		<if test="orderNo != null and orderNo!=''">
			AND a.orderNo like "%"#{orderNo}"%"
		</if>
		LIMIT
		#{pagination.pageIndex},#{pagination.pageSize}
	</select>
	
	<select id="getListByPage"
		parameterType="com.meida.front.pay.domain.dto.FundInListDto"
		resultMap="myPageResultMap">
		select
		a.fundInId,a.memberId,a.orderNo,a.inMoney,a.inDate,a.inWay,a.createDate,a.createUserId,a.createUser,a.updateDate,a.updateUserId,a.updateUser,a.isValid,a.remark
		from tb_fund_in as a
		where a.isValid=1
		<if test="memberId != null and memberId!=''">
			AND a.memberId like "%"#{memberId}"%"
		</if>
		<if test="orderNo != null and orderNo!=''">
			AND a.orderNo like "%"#{orderNo}"%"
		</if>
		LIMIT
		#{pagination.pageIndex},#{pagination.pageSize}
	</select>


	<select id="getTotalRecord"
		parameterType="com.meida.front.pay.domain.dto.FundInListDto"
		resultType="java.lang.Long">
		select count(*) from tb_fund_in
		where isValid=1
		<if test="memberId != null and memberId!=''">
			AND memberId like "%"#{memberId}"%"
		</if>
		<if test="orderNo != null and orderNo!=''">
			AND orderNo like "%"#{orderNo}"%"
		</if>
		LIMIT
		#{pagination.pageIndex},#{pagination.pageSize}
	</select>


	<insert id="save"
		parameterType="com.meida.front.pay.domain.po.FundIn">
		insert into tb_fund_in
		(memberId,orderNo,inMoney,inWay,inDate,createDate,createUserId,createUser,updateDate,updateUserId,updateUser,isValid,remark,signature)
		values
		(#{memberId},#{orderNo},#{inMoney},#{inWay},#{inDate},
		#{createDate},#{createUserId},#{createUser},#{updateDate},#{updateUserId},#{updateUser},#{isValid},#{remark},#{signature})

	</insert>


