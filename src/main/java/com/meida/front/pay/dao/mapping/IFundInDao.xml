		</set>
		where orderNo=#{orderNo}
	</update>


	<delete id="deletePhysicalById" parameterType="java.lang.Long">
		delete from
		tb_fund_in
		where fundInId=#{id}
	</delete>


	<delete id="deleteLogicById" parameterType="java.lang.Long">
		update
		tb_fund_in set
		isValid=-1 where fundInId=#{id}
	</delete>

</mapper>