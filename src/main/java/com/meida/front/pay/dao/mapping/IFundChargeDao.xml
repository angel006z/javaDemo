			<if test="updateDate!=null">
				updateDate=#{updateDate},
			</if>
			<if test="updateUserId!=null">
				updateUserId=#{updateUserId},
			</if>
			<if test="updateUser!=null">
				updateUser=#{updateUser},
			</if>
			<if test="remark!=null">
				remark=#{remark}
			</if>
			<if test="signature!=null">
				signature=#{signature}
			</if>
		</set>
		where orderNo=#{orderNo}
	</update>


