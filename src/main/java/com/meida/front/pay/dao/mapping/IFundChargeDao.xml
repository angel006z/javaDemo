<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org/DTD Mapper 3.0" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper
	namespace="com.meida.front.pay.dao.inter.IFundChargeDao">

	<resultMap id="myResultMap"
		type="com.meida.front.pay.domain.po.FundCharge">
		<id property="fundChargeId" column="fundChargeId"></id>
		<result property="memberId" column="memberId"></result>
		<result property="orderNo" column="orderNo"></result>
		<result property="chargeMoney" column="chargeMoney"></result>
		<result property="chargeDate" column="chargeDate"></result>
		<result property="payType" column="payType"></result>
		<result property="payChannel" column="payChannel"></result>
		<result property="isPay" column="isPay"></result>
		<result property="createDate" column="createDate"></result>
		<result property="createUserId" column="createUserId"></result>
		<result property="createUser" column="createUser"></result>
		<result property="updateDate" column="updateDate"></result>
		<result property="updateUserId" column="updateUserId"></result>
		<result property="updateUser" column="updateUser"></result>
		<result property="isValid" column="isValid"></result>
		<result property="remark" column="remark"></result>
	</resultMap>

	<select id="getObjectById" parameterType="java.lang.Long"
		resultMap="myResultMap">
		select
		fundChargeId,memberId,orderNo,chargeMoney,chargeDate,payType,payChannel,isPay,createDate,createUserId,createUser,updateDate,updateUserId,updateUser,isValid,remark
		from tb_fund_charge where fundChargeId=#{id}
	</select>
	
	<select id="getObjectByOrderNo" parameterType="java.lang.String"
		resultMap="myResultMap">
		select
		fundChargeId,memberId,orderNo,chargeMoney,chargeDate,payType,payChannel,isPay,createDate,createUserId,createUser,updateDate,updateUserId,updateUser,isValid,remark
		from tb_fund_charge where orderNo=#{orderNo}
	</select>
	
	<select id="isExistOrderNo" parameterType="java.lang.String"
		resultType="java.lang.Long">
		select count(*) from tb_fund_charge where
		orderNo=#{orderNo}
	</select>


	<select id="getListByAll" resultMap="myResultMap">
		select
		fundChargeId,memberId,orderNo,chargeMoney,chargeDate,payType,payChannel,isPay,createDate,createUserId,createUser,updateDate,updateUserId,updateUser,isValid,remark
		from tb_fund_charge
	</select>


	<select id="getListByValid" resultMap="myResultMap">
		select
		fundChargeId,memberId,orderNo,chargeMoney,chargeDate,payType,payChannel,isPay,createDate,createUserId,createUser,updateDate,updateUserId,updateUser,isValid,remark
		from tb_fund_charge where isValid=1
	</select>

	<resultMap id="myPageResultMap"
		type="com.meida.front.pay.domain.vo.FundChargeVo">
		<id property="fundChargeId" column="fundChargeId"></id>
		<result property="memberId" column="memberId"></result>
		<result property="orderNo" column="orderNo"></result>
		<result property="chargeMoney" column="chargeMoney"></result>
		<result property="chargeDate" column="chargeDate"></result>
		<result property="payType" column="payType"></result>
		<result property="payChannel" column="payChannel"></result>
		<result property="isPay" column="isPay"></result>
		<result property="createDate" column="createDate"></result>
		<result property="createUserId" column="createUserId"></result>
		<result property="createUser" column="createUser"></result>
		<result property="updateDate" column="updateDate"></result>
		<result property="updateUserId" column="updateUserId"></result>
		<result property="updateUser" column="updateUser"></result>
		<result property="isValid" column="isValid"></result>
		<result property="remark" column="remark"></result>
	</resultMap>
	
	<select id="getListWhereByPage"
		parameterType="com.meida.front.pay.domain.dto.FundChargeListDto"
		resultMap="myPageResultMap">
		select
		a.fundChargeId,a.memberId,a.orderNo,a.chargeMoney,a.chargeDate,a.payType,a.payChannel,a.isPay,a.createDate,a.createUserId,a.createUser,a.updateDate,a.updateUserId,a.updateUser,a.isValid,a.remark
		from tb_fund_charge as a
		where a.isValid=1
		<if test="memberId != null and memberId!=''">
			AND a.memberId like "%"#{memberId}"%"
		</if>
		<if test="orderNo != null and orderNo!=''">
			AND a.orderNo like "%"#{orderNo}"%"
		</if>
		LIMIT
		#{pagination.pageIndex},#{pagination.pageSize}
	</select>
	
	<select id="getListByPage"
		parameterType="com.meida.front.pay.domain.dto.FundChargeListDto"
		resultMap="myPageResultMap">
		select
		a.fundChargeId,a.memberId,a.orderNo,a.chargeMoney,a.chargeDate,a.payType,a.payChannel,a.isPay,a.createDate,a.createUserId,a.createUser,a.updateDate,a.updateUserId,a.updateUser,a.isValid,a.remark
		from tb_fund_charge as a
		where a.isValid=1
		<if test="memberId != null and memberId!=''">
			AND a.memberId like "%"#{memberId}"%"
		</if>
		<if test="orderNo != null and orderNo!=''">
			AND a.orderNo like "%"#{orderNo}"%"
		</if>
		LIMIT
		#{pagination.pageIndex},#{pagination.pageSize}
	</select>


	<select id="getTotalRecord"
		parameterType="com.meida.front.pay.domain.dto.FundChargeListDto"
		resultType="java.lang.Long">
		select count(*) from tb_fund_charge
		where isValid=1
		<if test="memberId != null and memberId!=''">
			AND memberId like "%"#{memberId}"%"
		</if>
		<if test="orderNo != null and orderNo!=''">
			AND orderNo like "%"#{orderNo}"%"
		</if>
		LIMIT
		#{pagination.pageIndex},#{pagination.pageSize}
	</select>


	<insert id="save"
		parameterType="com.meida.front.pay.domain.po.FundCharge">
		insert into tb_fund_charge
		(memberId,orderNo,chargeMoney,payType,chargeDate,payChannel,isPay,createDate,createUserId,createUser,updateDate,updateUserId,updateUser,isValid,remark,signature)
		values
		(#{memberId},#{orderNo},#{chargeMoney},#{payType},#{chargeDate},
		#{payChannel},#{isPay},#{createDate},#{createUserId},#{createUser},#{updateDate},#{updateUserId},#{updateUser},#{isValid},#{remark},#{signature})

