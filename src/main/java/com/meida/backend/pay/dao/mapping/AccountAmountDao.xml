    <select id="getObjectById" parameterType="java.lang.String"
            resultMap="myResultMap">
		select
                a.account_amount_id,
                a.member_id,
                a.total_amount,
                a.create_date,
                a.create_user_id,
                a.create_user,
                a.update_date,
                a.update_user_id,
                a.update_user,
                a.is_valid,
                a.remark,
                a.signature		from tb_account_amount as a where	a.account_amount_id=#{id}
	</select>

    <select id="getListPage"  parameterType="com.meida.backend.pay.dto.AccountAmountListDto"
